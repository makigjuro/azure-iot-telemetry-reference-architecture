{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    }
  },
  "IpRateLimiting": {
    "GeneralRules": [
      {
        "Endpoint": "*:/api/events/*",
        "Period": "1m",
        "Limit": 1000
      }
    ]
  }
}
