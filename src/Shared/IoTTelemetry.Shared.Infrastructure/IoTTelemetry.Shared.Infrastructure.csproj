<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <!-- Suppress analyzers for intentional design decisions -->
    <!-- CA1000: Static factory methods in Result<T> pattern -->
    <!-- CA1716: "Shared" in namespace and "Error" type name are acceptable -->
    <NoWarn>$(NoWarn);CA1000;CA1716</NoWarn>
  </PropertyGroup>

</Project>
